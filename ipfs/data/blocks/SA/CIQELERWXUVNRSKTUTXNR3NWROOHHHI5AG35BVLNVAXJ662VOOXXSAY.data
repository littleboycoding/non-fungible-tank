
µM≠M__NUXT_JSONP__("/legal", (function(a,b,c,d,e,f,g,h,i,j,k,l,m,n,o,p,q,r,s,t,u,v,w,x,y,z,A,B,C,D,E,F,G,H,I,J){return {data:[{legal:{slug:"legal",description:"Report copyright infringement on the IPFS Gateway Service.",title:"Legal",toc:[{id:o,depth:g,text:p},{id:q,depth:g,text:r},{id:s,depth:g,text:t},{id:u,depth:g,text:v},{id:w,depth:g,text:x},{id:y,depth:g,text:z}],body:{type:"root",children:[{type:b,tag:d,props:{},children:[{type:a,value:"Protocol Labs Inc. and the core IPFS team have adopted this copyright infringement policy in accordance with the "},{type:b,tag:f,props:{href:"http:\u002F\u002Fhttp:\u002F\u002Flcweb.loc.gov\u002Fcopyright\u002Flegislation\u002Fdmca.pdf",rel:[A,B,C],target:D},children:[{type:a,value:"Digital Millennium Copyright Act"}]},{type:a,value:E}]},{type:a,value:c},{type:b,tag:d,props:{},children:[{type:a,value:"If you are a copyright owner, or agent of the owner, and you believe that content available on the IPFS Gateway Service hosted at "},{type:b,tag:n,props:{},children:[{type:a,value:F}]},{type:a,value:" or "},{type:b,tag:n,props:{},children:[{type:a,value:G}]},{type:a,value:" infringes one or more of your copyrights please notify us by sending a DMCA Notice of the infringement containing the information below.    After we receive a valid and complete notice, we will investigate and block or otherwise make the material unavailable through the IPFS Gateway Service. Learn more about our process below."}]},{type:a,value:c},{type:b,tag:d,props:{},children:[{type:b,tag:H,props:{},children:[{type:a,value:"Please note - this policy is only applicable to the IPFS Gateway Service operated by the IPFS team (including "},{type:b,tag:n,props:{},children:[{type:a,value:F}]},{type:a,value:" & "},{type:b,tag:n,props:{},children:[{type:a,value:G}]},{type:a,value:"). See "},{type:b,tag:f,props:{href:"https:\u002F\u002Fipfs.github.io\u002Fpublic-gateway-checker\u002F",rel:[A,B,C],target:D},children:[{type:a,value:"the public gateway checker"}]},{type:a,value:" for owners of other gateways to contact directly."}]}]},{type:a,value:c},{type:b,tag:d,props:{},children:[{type:a,value:"The email address (preferred) and physical address (less preferred) of our Designated Agent to receive notification of claimed infringement is listed at the end of this policy.  Please read it carefully and make sure you are aware of the legal implications of the DMCA. You can be held liable for damages, including costs and attorney fees, if you materially misrepresent that material or activity infringes on your copyright.  We may highlight such abuses, too. Consult an attorney before filing a DMCA Notice if you are unsure whether copyrights have been infringed, or if you are unsure whether your materials have been removed in error."}]},{type:a,value:c},{type:b,tag:h,props:{id:o},children:[{type:b,tag:f,props:{href:"#how-do-you-file-a-dmca-notice",ariaHidden:i,tabIndex:j},children:[{type:b,tag:k,props:{className:[l,m]},children:[]}]},{type:a,value:p}]},{type:a,value:c},{type:b,tag:d,props:{},children:[{type:a,value:"Your DMCA Notice must include all of the following information:"}]},{type:a,value:c},{type:b,tag:I,props:{},children:[{type:a,value:c},{type:b,tag:e,props:{},children:[{type:a,value:"A physical or electronic signature of the copyright owner, or a person authorized to act on behalf of the copyright owner;"}]},{type:a,value:c},{type:b,tag:e,props:{},children:[{type:a,value:"Identification (URL) of the copyrighted work or material being infringed upon;"}]},{type:a,value:c},{type:b,tag:e,props:{},children:[{type:a,value:"Description of the work or material that you claim to be infringing and would like to be removed, including information regarding its location (URL) with enough detail so that we can and verify it;"}]},{type:a,value:c},{type:b,tag:e,props:{},children:[{type:a,value:"Your full legal name, mailing address, telephone number, and email address where we can contact you;"}]},{type:a,value:c},{type:b,tag:e,props:{},children:[{type:a,value:"A statement that you have a good faith belief that use of the material in the manner complained of is not authorized by the copyright owner, its agent, or the law; and"}]},{type:a,value:c},{type:b,tag:e,props:{},children:[{type:a,value:"A statement that the information in your DMCA Notice is accurate, and under penalty of perjury, that you are the copyright owner or are authorized to act on behalf of the copyright owner."}]},{type:a,value:c}]},{type:a,value:c},{type:b,tag:h,props:{id:q},children:[{type:b,tag:f,props:{href:"#where-do-you-emailsend-the-dmca-notice",ariaHidden:i,tabIndex:j},children:[{type:b,tag:k,props:{className:[l,m]},children:[]}]},{type:a,value:r}]},{type:a,value:c},{type:b,tag:d,props:{},children:[{type:a,value:"The quickest way is to email it to "},{type:b,tag:f,props:{href:"mailto:abuse@ipfs.io"},children:[{type:a,value:"abuse@ipfs.io"}]},{type:a,value:". If you prefer, you can instead send it to:"}]},{type:a,value:c},{type:b,tag:"div",props:{className:["nuxt-content-highlight"]},children:[{type:b,tag:"pre",props:{className:["language-text","line-numbers"]},children:[{type:b,tag:n,props:{},children:[{type:a,value:"Protocol Labs Inc.\nAttn: IPFS Designated DMCA Agent\n427 N. Tatnall St. #51207\nWilmington, Delaware 19801\n"}]}]}]},{type:a,value:c},{type:b,tag:d,props:{},children:[{type:a,value:"Our Designated Agent‚Äôs telephone number is "},{type:b,tag:H,props:{},children:[{type:a,value:"+1 (302) 409-0239"}]},{type:a,value:E}]},{type:a,value:c},{type:b,tag:h,props:{id:s},children:[{type:b,tag:f,props:{href:"#what-happens-after-sending-the-dmca-notice",ariaHidden:i,tabIndex:j},children:[{type:b,tag:k,props:{className:[l,m]},children:[]}]},{type:a,value:t}]},{type:a,value:c},{type:b,tag:d,props:{},children:[{type:a,value:"If the DMCA Notice complies with the above requirements, we will remove or disable access to the cited content that is allegedly infringing. We will make a good faith attempt to notify the alleged infringer of the takedown, with a copy of your DMCA Notice, using the contact information provided to us."}]},{type:a,value:c},{type:b,tag:h,props:{id:u},children:[{type:b,tag:f,props:{href:"#what-happens-if-you-receive-a-dmca-notification-from-us",ariaHidden:i,tabIndex:j},children:[{type:b,tag:k,props:{className:[l,m]},children:[]}]},{type:a,value:v}]},{type:a,value:c},{type:b,tag:d,props:{},children:[{type:a,value:"If you believe your content was removed in error, you can file a counter notification with the information below (‚ÄúDMCA Counter Notification‚Äù)."}]},{type:a,value:c},{type:b,tag:h,props:{id:w},children:[{type:b,tag:f,props:{href:"#how-do-you-file-a-dmca-counter-notification",ariaHidden:i,tabIndex:j},children:[{type:b,tag:k,props:{className:[l,m]},children:[]}]},{type:a,value:x}]},{type:a,value:c},{type:b,tag:d,props:{},children:[{type:a,value:"Your DMCA Counter Notification must include all of the following information:"}]},{type:a,value:c},{type:b,tag:I,props:{},children:[{type:a,value:c},{type:b,tag:e,props:{},children:[{type:a,value:"Your physical or electronic signature;"}]},{type:a,value:c},{type:b,tag:e,props:{},children:[{type:a,value:"Identification (URL) of the material that has been removed or to which access has been disabled and the location at which the material appeared before it was removed or access to it was disabled (the description from the DMCA Notice is acceptable);"}]},{type:a,value:c},{type:b,tag:e,props:{},children:[{type:a,value:"A statement under penalty of perjury that you have a good faith belief that the material was removed or disabled as a result of mistake or misidentification of the material to be removed or disabled;"}]},{type:a,value:c},{type:b,tag:e,props:{},children:[{type:a,value:"Your full legal name, mailing address, telephone number, and email address where we can contact you; and"}]},{type:a,value:c},{type:b,tag:e,props:{},children:[{type:a,value:"A statement that you consent to the jurisdiction of Federal District Court for the judicial district in which your address is located, or if your address is outside of the United States, for any judicial district in which Protocol Labs may be found, and that you will accept service of process from the person who provided the DMCA Notice or an agent of such person."}]},{type:a,value:c}]},{type:a,value:c},{type:b,tag:h,props:{id:y},children:[{type:b,tag:f,props:{href:"#what-happens-after-sending-the-dmca-counter-notification",ariaHidden:i,tabIndex:j},children:[{type:b,tag:k,props:{className:[l,m]},children:[]}]},{type:a,value:z}]},{type:a,value:c},{type:b,tag:d,props:{},children:[{type:a,value:"If we receive valid DMCA Counter Notification that meets the above criteria, we will forward a copy to the person who filed the original DMCA Notice. If we do not receive notice within 10 business days that the person who submitted the DMCA Notice is seeking a court order to prevent the infringement of the content at issue, we will replace or re-enable access to the content that was removed."}]}]},dir:"\u002F",path:"\u002Flegal",extension:".md",createdAt:J,updatedAt:J}}],fetch:{},mutations:[]}}("text","element","\n","p","li","a",3,"h3","true",-1,"span","icon","icon-link","code","how-do-you-file-a-dmca-notice","How do you file a DMCA Notice?","where-do-you-emailsend-the-dmca-notice","Where do you email\u002Fsend the DMCA Notice?","what-happens-after-sending-the-dmca-notice","What happens after sending the DMCA Notice?","what-happens-if-you-receive-a-dmca-notification-from-us","What happens if you receive a DMCA Notification from us?","how-do-you-file-a-dmca-counter-notification","How do you file a DMCA Counter Notification?","what-happens-after-sending-the-dmca-counter-notification","What happens after sending the DMCA Counter Notification?","nofollow","noopener","noreferrer","_blank",".","ipfs.io","dweb.link","strong","ol","2021-11-22T12:12:56.678Z")));≠M